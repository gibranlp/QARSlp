rm -rf ~/.config/wpg/schemes/*
pkill dunst &  
picom & 
cp ~/.cache/wal/dunstrc ~/.config/dunst/dunstrc &
cp ~/.cache/wal/config ~/.config/cava/config &
pkill -USR1 cava &
dunst &
wal-telegram &
wal -R
